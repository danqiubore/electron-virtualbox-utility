{
  "name": "electron-virtualbox-utility",
  "version": "1.0.0",
  "description": "html packager,打包成一个exe项目，或者安装包",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "debug": "electron --inspect=5858 .",
    "build-mac": "electron-builder --mac",
    "build-win": "electron-builder --win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danqiubore/electron-virtualbox-utility.git"
  },
  "keywords": [
    "virtualbox",
    "enigma",
    "htmlpackager"
  ],
  "build": {
    "appId": "yg.course",
    "productName": "课件展示",
    "copyright": "1.0.0",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "src/images/icon_course.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "src/images/icon_course.icns",
      "artifactName": "${productName}_setup_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true
    }
  },
  "author": "danqiubore",
  "license": "ISC",
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "iconv-lite": "^0.6.2",
    "node-cmd": "^4.0.0"
  },
  "dependencies": {
    "iconv-lite": "^0.6.2",
    "node-cmd": "^4.0.0"
  }
}
